{
	"name": "wakatimejs",
	"version": "2.0.0",
	"license": "MIT",
	"description": "Type safe JavaScript client for the WakaTime REST API",
	"main": "dist/node/index.js",
	"types": "dist/node/index.d.ts",
	"browser": "dist/browser/bundle.js",
	"keywords": [
		"wakatime",
		"api",
		"client",
		"javascript",
		"typescript"
	],
	"scripts": {
