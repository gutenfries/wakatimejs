		"esbuild": "^0.19.5",
		"esbuild-plugin-d.ts": "^1.1.0",
		"eslint": "^8.52.0",
		"eslint-config-prettier": "^9.0.0",
		"estrella": "^1.4.1",
		"jest": "^29.7.0",
		"prettier": "^3.0.3",
		"ts-jest": "^29.1.1",
		"ts-node": "^10.9.1",
		"typescript": "^5.2.2"
	},
	"dependencies": {
		"axios": "^1.5.1",
		"dotenv": "^16.3.1"
